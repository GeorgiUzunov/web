//Title of this code

#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main(int argc, char **argv)
{
    int i = 0;
    
    srand(time(NULL));
    //cout << time(NULL) << "\n";
    for (;i < 5; i++) cout << rand() % 100 + 1 << "\t";
    
    return EXIT_SUCCESS;
}

