#include <studio.h>
int main() {
    
    int i;
    int sum=0;
    
    i=0;
    while (i<100){
        
        i=i+1;
        sum=sum+i;
    }
    
    fprintf(stdout,"%d\n", sum);
    
 return 0;
}

//Title of this code

#include <studio.h>

int main() {
    int age;
    
    for (;;) {
    fprintf(stdout, "Type Your Age: ");
    fscanf(stdin, "%d",&age);
    }
    
    if (age>=0 && age<=3) {
    fprintf(stdout, "Baby\n");
    }

    else if (age>=4 && <=7){
    fprintf(stdout, "Kindergarden\n");
    }
    else if (age>7 && <=18){
    fprintf(stdout, "School\n");
    }
    else if (age>18 && <=25){
    fprintf(stdout, "Uni\n");
    }
    else if (age>25){
    fprintf(stdout, "Work\n");
    }
 return 0;
}
