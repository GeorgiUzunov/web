CREATE TABLE `persons` (
	`ID` INT(11) NOT NULL AUTO_INCREMENT,
	`CustomersName` VARCHAR(255) NOT NULL COLLATE 'utf8_bin',
	`CustomersID` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`Address` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`City` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`OrderNom` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`OrderDate` DATE NULL DEFAULT NULL,
	`Duantiti` FLOAT NULL DEFAULT NULL,
	`Price` DECIMAL(10,0) NULL DEFAULT NULL,
	PRIMARY KEY (`ID`)
)
COLLATE='utf8_bin'
ENGINE=MyISAM;
