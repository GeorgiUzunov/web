CREATE TABLE кола_grozdan (
ID INT(11) NOT NULL AUTO_INCREMENT,
модел  VARCHAR(255) NOT NULL ,
цвят  VARCHAR(255) NOT NULL,
мощност int NOT NULL,
скоростна_кутия VARCHAR(255) NOT NULL,
тип_двигател VARCHAR(255) NOT NULL,
град VARCHAR(255) NOT NULL,
PRIMARY KEY (ID)
)
COLLATE='utf8_general_ci'
ENGINE=MyISAM
INSERT INTO кола_grozdan (модел, цвят, мощност,скоростна_кутия,тип_двигател,град),
 VALUES ('хонда','бял','75','ръчна','дизел','софия'),
        ('хонда','син','140','автоматична','бензин','бургас');
        

 
