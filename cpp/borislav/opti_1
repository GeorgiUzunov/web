#include <iostream>

using namespace std;
int main() {

int mas[10][11];
int d;
cout << "Vuvedete broi na chislata=";
cin >> d;

for (int i=0;i<d;d++){

	cin >>	mas[i][11] ;
}
	for (int i=0 ; i<d ; d++){
		for (int j=0 ; j<10; j++){
	
		if (mas[i][11]<10-j) 
		mas[i][j]=1;
	
		else
		mas[i][j]=0;
	}
	}
	 for (int j=0 ; j<10 ; j++){
		for (int i=0 ; i<d; i++)
		if (mas[i][j]=1) 
		cout << "*";
}
	return 0;
}
