#include <iostream>
#include <math.h>

using namespace std;

int main()

{
	char ch = '@' + 20;
	int chislo = 11;
	cout <<"Hello word"
	<< endl << (1 << chislo + 2);

	return 0;
 }


#include <iostream>
#include <math.h>

using namespace std;

int main()

{
	char ch = '@' + 20;
	int chislo = 11;
	cout <<"Hello word"
	<< endl << (1 << chislo + 2);
	cout << "Enter a number :";
   cin>>chislo;
   cout << "Number" << chislo << "^2 =" << chislo*chislo <<endl;
   
	return 0;
 }
