#include <iostream>

using namespace std;

int main()

{
	cout<< "Hello, Mel!" << endl;
	return 0;
 }


#include <iostream>

using namespace std;

int main()

{
	char ch = '@' + 10;
	cout <<"Hello!Mel"
	<< endl << (int) ch << endl;
	return 0;
 }

#include <iostream>

using namespace std;

int main()

{
	char ch = '@' + 10;
	cout <<"Hello!Mel"
	<< endl << ch << endl;
	return 0;
 }

#include <iostream>

using namespace std;

int main()

{
	char ch = '@' + 10;
	int chislo = 11;
	cout <<"Hello!Mel"
	<< endl << (int) ch << endl << (chislo+200)*16 <<endl;
	return 0;
 }
 


#include <iostream>
#include <math.h>

using namespace std;

int main()

{
	char ch = '@' + 10;
	int chislo = 11;
	cout <<"Hello!Mel"
	<< endl << (1 << chislo + 200);
	return 0;
 }
 
#include <iostream>
#include <math.h>

using namespace std;

int main()

{
	char ch = '@' + 20;
	int chislo = 11;
	cout <<"Hello, Mel!"
	<< endl << (1 << chislo + 2);
	cout << "Vavedete stoynost";
	cin >> chislo;
	cout << "Number" << chislo << "^2 =" << chislo*chislo << endl;
	return 0;
 }
 
