i=5;

while (i='0');

fprintf(stdout,"OK");

}

/* _ raboti.. */


#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

fprintf(stdout, "%d\n", i = 0);

while (i=0);

fprintf(stdout,"OK");

}
/* 0 i OK  */


#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

fprintf(stdout, "%d\n", i = '0');

while (i='0');

fprintf(stdout,"OK");

}
/* O48 koda na 0 */


Random number generator CPP use 'rand' +  #include <stdlib.h> i #include <time.h> za time



#include <iostream>
#include <stdio.h>
#include <math.h>
#include <stdlib.h>

using namespace std;

int main ()

{
int chislo, i;

1 + rand()%101;

fprintf(stdout,"%d",rand);
}

/* 4281800 */

#include <iostream>
#include <stdio.h>
#include <math.h>
#include <stdlib.h>

using namespace std;

int main ()

{
int chislo, i;

rand()%100;

fprintf(stdout,"%d",rand);
}

/* 4281768 */

#include <iostream>
#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <time.h>

using namespace std;

int main ()
{
int chislo, i;

srand(time(NULL));

rand()%100;

fprintf(stdout,"%d",(time(NULL)));

}

