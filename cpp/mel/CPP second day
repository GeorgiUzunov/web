#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

while (i!=0);

fprintf(stdout,"OK");

}


#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

while (i);

fprintf(stdout,"OK");

}

#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

while (i-5);

fprintf(stdout,"OK");

}

