#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

while (i!=0);

fprintf(stdout,"OK");

}

/* proba s ne e ravno na 0*/

#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

while (i);

fprintf(stdout,"OK");

}
/* ne se poluchava s 5 */


#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=5;

while (i-5);

fprintf(stdout,"OK");

}
/* taka se izpylnawa i dava 0k */


#include <iostream>
#include <stdio.h>
#include <math.h>

using namespace std;

int main ()

{
int i;

i=0;

while (i);

fprintf(stdout,"OK");

}
/* taka se izpylnawa i dava 0k */


